{
    "version": 2,
    "buildCommand": "npx mastra build && next build",
    "installCommand": "npm install",
    "framework": "nextjs",
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/next"
        }
    ],
    "rewrites": [
        {
            "source": "/api/agents/:path*",
            "destination": "/api/agents/:path*"
        }
    ]
}